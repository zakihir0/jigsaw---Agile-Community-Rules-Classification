   "cell_type": "code",
   "execution_count": 3,
   "id": "e0cac4f7",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-10-15T01:52:54.674203Z",
     "iopub.status.busy": "2025-10-15T01:52:54.673851Z",
     "iopub.status.idle": "2025-10-15T01:52:54.678428Z",
     "shell.execute_reply": "2025-10-15T01:52:54.677743Z"
    },
    "papermill": {
     "duration": 0.010046,
     "end_time": "2025-10-15T01:52:54.679416",
     "exception": false,
     "start_time": "2025-10-15T01:52:54.669370",
     "status": "completed"
